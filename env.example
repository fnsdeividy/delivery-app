# Configurações da API Cardapio.IO Delivery
NEXT_PUBLIC_CARDAPIO_API_URL=http://localhost:3001/api/v1

# Configurações do Next.js
NEXTAUTH_URL=http://localhost:3001
NEXTAUTH_SECRET=your-secret-key-here

# Configurações do banco de dados (se necessário)
# DATABASE_URL=postgresql://user:password@localhost:5432/delivery_app

# Configurações de desenvolvimento
NODE_ENV=development

# Configurações de build
NEXT_PUBLIC_APP_NAME=Cardapio.IO Delivery
NEXT_PUBLIC_APP_VERSION=1.0.0

# Configurações de analytics (opcional)
# NEXT_PUBLIC_GA_ID=your-ga-id
# NEXT_PUBLIC_GTM_ID=your-gtm-id

# Configurações de upload (opcional)
# NEXT_PUBLIC_UPLOAD_URL=http://localhost:3001/api/v1/upload
# NEXT_PUBLIC_MAX_FILE_SIZE=5242880

# Configurações de cache
NEXT_PUBLIC_CACHE_TTL=300000
NEXT_PUBLIC_QUERY_STALE_TIME=60000 